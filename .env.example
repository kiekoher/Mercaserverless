# Supabase project credentials
# These can be found in your Supabase project settings under API
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_KEY=YOUR_SUPABASE_SERVICE_KEY # Server-side only, keep secret

# Google Gemini API Key
# Can be found on your Google AI Studio dashboard
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# URL de conexión a Redis para rate limiting
REDIS_URL=redis://localhost:6379

# Configuración de logs
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log
LOG_MAX_SIZE=10485760
LOG_MAX_FILES=5

# IDs de usuarios de prueba para Cypress
CYPRESS_ADMIN_ID=
CYPRESS_SUPERVISOR_ID=
CYPRESS_MERCADERISTA_ID=
