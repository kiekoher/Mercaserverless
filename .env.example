# Example environment variables
# Replace placeholders with secure values.
# Do not commit real secrets. Rotate credentials regularly and store them securely.

# Supabase project credentials
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_KEY=

# Google API keys
GEMINI_API_KEY=
GOOGLE_MAPS_API_KEY=

# Serverless Redis for Rate Limiting (e.g., Upstash)
UPSTASH_REDIS_URL=
# Set to true to allow requests when Redis is down (not recommended for production)
RATE_LIMIT_FAIL_OPEN=true

# Logging configuration
LOG_LEVEL=info
# Logtail source token for serverless logging
LOGTAIL_SOURCE_TOKEN=

# PostgreSQL credentials for docker-compose
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=

# Geocoding configuration
GEOCODE_TIMEOUT_MS=1000
GEOCODE_RETRIES=3

# Test user IDs for Cypress
CYPRESS_ADMIN_ID=
CYPRESS_SUPERVISOR_ID=
CYPRESS_MERCADERISTA_ID=

