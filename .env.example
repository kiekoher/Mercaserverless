# Example environment variables
# Replace placeholders with secure values.
# Do not commit real secrets. Rotate credentials regularly and store them securely.

# Supabase project credentials
NEXT_PUBLIC_SUPABASE_URL="https://example.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="example-anon-key"
SUPABASE_SERVICE_KEY="example-service-key"

# Google API keys
GEMINI_API_KEY="example-gemini-key"
GOOGLE_MAPS_API_KEY="example-maps-key"

# AI configuration
AI_TIMEOUT_MS=10000

# Serverless Redis for Rate Limiting (e.g., Upstash)
UPSTASH_REDIS_REST_URL="https://example-redis.upstash.io"
UPSTASH_REDIS_REST_TOKEN="example-rest-token"
UPSTASH_REDIS_URL=
RATE_LIMIT_FAIL_OPEN=false

# Logging configuration
LOG_LEVEL=info
# Logtail source token for serverless logging
LOGTAIL_SOURCE_TOKEN=

# Geocoding configuration
GEOCODE_TIMEOUT_MS=1000
GEOCODE_RETRIES=3
GEOCODE_CONCURRENCY=5
GEOCODE_RETRY_BASE_MS=100

# Test user IDs for Cypress
CYPRESS_ADMIN_ID=
CYPRESS_SUPERVISOR_ID=
CYPRESS_MERCADERISTA_ID=
NEXT_PUBLIC_BYPASS_AUTH_FOR_TESTS=false

# Health check token
HEALTHCHECK_TOKEN=
