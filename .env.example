# Supabase project credentials
# These can be found in your Supabase project settings under API
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_KEY=YOUR_SUPABASE_SERVICE_KEY # Server-side only, keep secret

# Google Gemini API Key
# Can be found on your Google AI Studio dashboard
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY
ENABLE_E2E_LOGIN=false

# URL de conexi√≥n a Redis para rate limiting
REDIS_URL=redis://localhost:6379
RATE_LIMITER_FAIL_OPEN=false

# Nivel de detalle para los logs (pino)
LOG_LEVEL=info
